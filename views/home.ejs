<link rel="stylesheet" href="./css/home.css">
    <main>        
        <section>
            <h4>Posts</h4>
            <form action="/post/create" method="post" id="post-form">
                <textarea name="content" id="post-textarea" cols="30" rows="10">
                    
                </textarea>
                <button type="submit">Submit</button>
            </form>
        </section>
        <section>
            <h4>All Posts</h4>
            <ol id="ol-posts">
                <%for( post of posts){%>
               <%- include("_post") %>
               <%}%>
            </ol>
        </section>
        
        <section class="all-user">
            <h1>All Users List</h1>
            <ul>
                <%for(user of users){%>
                    <li>
                        <a href="user/profile/<%= user.id%>"><%= user.name%></a>
                    </li>
                    <%}%>
                </ul>
            </section>
        </main>

<script src="js/home.js"></script>